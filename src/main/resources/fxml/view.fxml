<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane fx:id="view" prefHeight="200.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <ListView fx:id="nombresList" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
   <bottom>
      <HBox fx:id="hboxButt" alignment="BASELINE_RIGHT" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="añadirButt" mnemonicParsing="false" onAction="#OnañadirAction" text="Añadir">
               <HBox.margin>
                  <Insets left="3.0" right="3.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="quitarButt" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#OnquitarAction" text="Quitar">
               <HBox.margin>
                  <Insets left="3.0" right="3.0" />
               </HBox.margin>
            </Button>
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
   </bottom>
</BorderPane>
